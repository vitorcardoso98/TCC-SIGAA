<html>
	<head>
		<meta charset="UTF-8">
		<title>Cadastar Parâmetros</title>
		<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
	</head>
	
	<body>
		<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
		<form action="/cadastrar" method="POST">
		  <div class="form-group">
			<label for="codigo">Código</label>
			<input type="text" class="form-control" id="codigo" placeholder="Código" name="codigo">
		  </div>
		  <div class="form-group">
			<label for="descricao">Descrição</label>
			<input type="text" class="form-control" id="descricao" placeholder="Descrição" name="descricao">
		  </div>
		  <div class="form-group">
			<label for="valor">Código</label>
			<input type="text" class="form-control" id="valor" placeholder="Valor" name="valor">
		  </div>
		  <button type="submit" class="btn btn-primary">Cadastar</button>
		</form>
	</body>
</html>